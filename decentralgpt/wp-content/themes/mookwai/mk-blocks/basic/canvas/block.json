{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mk-block/canvas",
  "title": "Canvas",
  "version": "0.1.0",
  "category": "mk-b-cat-basic",
  "description": "The button can be set to open a fixed link or trigger a popup.",
  "keywords": ["mookwai", "section", "layout", "container", "grid", "flex"],
  "textdomain": "mookwai",
  "attributes": {
    "blockID": {
      "type": "string"
    },
    "globalSlug": {
      "type": "string"
    },
    "cssClass": {
      "type": "array"
    },
    "htmlAttr": {
      "type": "array"
    },
    "anchor": {
      "type": "string"
    },
    "isHide": {
      "type": "array"
    },
    "interactPreview": {
      "type": "boolean",
      "default": false
    }
	},
  "usesContext": [ "parentIsPreview", "parentHasLink" ],
  "supports": {
		"html": false
	}
}
