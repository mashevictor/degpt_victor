{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mk-block/post-content",
  "title": "文章内容",
  "version": "1.0",
  "category": "mk-b-cat-site",
  "description": "Basic containers for page sections",
  "keywords": ["mookwai", "content", "post", "page"],
  "textdomain": "mookwai",
  "attributes": {
    "blockID": {
      "type": "string"
    },
    "globalSlug": {
      "type": "string"
    },
    "cssClass": {
      "type": "array"
    },
    "htmlAttr": {
      "type": "array"
    },
    "anchor": {
      "type": "string"
    },
    "isHide": {
      "type": "array"
    },
    "tagName": {
      "type": "string",
      "default": "div"
    },
    "interactPreview": {
      "type": "boolean",
      "default": false
    }
	},
  "usesContext": [ "postId", "postType", "queryId" ],
  "supports": {
		"html": false
	}
}
