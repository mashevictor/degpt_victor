{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mk-block/horizontal",
  "title": "水平滚动",
  "version": "1.0",
  "category": "mk-b-cat-extention",
  "description": "Basic containers for page sections",
  "keywords": ["mookwai", "section", "layout", "container", "grid", "flex"],
  "textdomain": "mookwai",
  "attributes": {
    "blockID": {
      "type": "string"
    },
    "globalSlug": {
      "type": "string"
    },
    "cssClass": {
      "type": "array"
    },
    "htmlAttr": {
      "type": "array"
    },
    "anchor": {
      "type": "string"
    },
    "isHide": {
      "type": "array"
    },
    "tagName": {
      "type": "string",
      "default": "div"
    },
    "devices": {
      "type": "array",
      "default": ["Desktop", "Tablet", "Mobile"]
    },
    "interactPreview": {
      "type": "boolean",
      "default": false
    }
	},
  "providesContext": {
    "parentIsPreview": "interactPreview"
  },
  "usesContext": [ "parentIsPreview" ],
  "supports": {
		"html": false
	}
}
