{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mk-block/slides",
  "version": "1.0",
  "title": "滑块",
  "category": "mk-b-cat-extention",
  "description": "Basic containers for page sections",
  "keywords": ["mookwai", "section", "layout", "container", "grid", "flex"],
  "textdomain": "mookwai",
  "attributes": {
    "allowedBlocks": {
      "type": "array",
      "default": [ "mk-block/slides-container", "mk-block/slider-pagination", "mk-block/slider-toggle-group" ]
    },
    "blockID": {
      "type": "string"
    },
    "globalSlug": {
      "type": "string",
      "default": ""
    },
    "cssClass": {
      "type": "array"
    },
    "htmlAttr": {
      "type": "array"
    },
    "anchor": {
      "type": "string"
    },
    "isHide": {
      "type": "array"
    },
    "tagName": {
      "type": "string",
      "default": "div"
    },
    "interactPreview": {
      "type": "boolean",
      "default": false
    }
	},
  "providesContext": {
    "parentIsPreview": "interactPreview"
  },
  "usesContext": [ "parentIsPreview" ],
  "supports": {
		"html": false
	}
}
